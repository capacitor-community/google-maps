{
  "name": "@capacitor-community/google-maps",
  "version": "2.0.0-beta.7",
  "description": "Plugin using native Maps API for Android and iOS.",
  "main": "dist/esm/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/esm/index.d.ts",
  "scripts": {
    "docgen": "docgen --api CapacitorGoogleMapsPlugin --output-readme ./docs/api.md",
    "build": "npm run clean && tsc",
    "clean": "rimraf ./dist",
    "watch": "tsc --watch",
    "prepublishOnly": "npm run build",
    "prepare": "npm run build"
  },
  "author": "@capacitor-community",
  "license": "MIT",
  "devDependencies": {
    "@capacitor/android": "^4.0.0",
    "@capacitor/core": "^4.0.0",
    "@capacitor/docgen": "git+https://github.com/DutchConcepts/capacitor-docgen.git",
    "@capacitor/ios": "^4.0.0",
    "all-contributors-cli": "^6.20.0",
    "rimraf": "^3.0.0",
    "typescript": "~4.1.5"
  },
  "peerDependencies": {
    "@capacitor/core": "^4.0.0"
  },
  "files": [
    "dist/",
    "ios/",
    "android/",
    "CapacitorCommunityGoogleMaps.podspec"
  ],
  "keywords": [
    "capacitor",
    "plugin",
    "native"
  ],
  "capacitor": {
    "ios": {
      "src": "ios"
    },
    "android": {
      "src": "android"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/capacitor-community/google-maps.git"
  },
  "bugs": {
    "url": "https://github.com/capacitor-community/google-maps/issues"
  },
  "homepage": "https://github.com/capacitor-community/google-maps#readme",
  "directories": {
    "doc": "docs"
  }
}
